{
    "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
    "schemaVersion": 2,
    "services": [
        {
            "name": "qbitwebui",
            "image": "ghcr.io/maciejonos/qbitwebui:2.31.0",
            "isMain": true,
            "internalPort": 3000,
            "environment": [
                {
                    "key": "ENCRYPTION_KEY",
                    "value": "${ENCRYPTION_KEY}"
                }
            ],
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/data/data",
                    "containerPath": "/data"
                }
            ],
            "dependsOn": []
        }
    ]
}