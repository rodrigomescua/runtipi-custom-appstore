{
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "schemaVersion": 2,
  "services": [
    {
      "name": "freshrss-image-cache",
      "image": "ghcr.io/s373r/freshrss-image-cache-service-rs:0.2.6",
      "isMain": true,
      "internalPort": 3000,
      "environment": [
        {
          "key": "APP_PORT",
          "value": "3000"
        },
        {
          "key": "APP_ACCESS_TOKEN",
          "value": "${APP_ACCESS_TOKEN}"
        },
        {
          "key": "APP_IMAGES_DIR",
          "value": "/usr/src/app/images"
        },
        {
          "key": "APP_NO_ANSI_COLORS",
      "healthCheck": {
        "test": "curl -f http://localhost:3000/health || exit 1",
        "interval": "30s",
        "timeout": "10s",
        "retries": 3
      }healthCheck": {
        "test": "curl -f http://localhost:3000/health || exit 1",
        "interval": "30s",
        "timeout": "10s",
        "retries": 3
      }
    }
  ]
}
