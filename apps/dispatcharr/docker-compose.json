{
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "schemaVersion": 2,
  "services": [
    {
      "name": "dispatcharr",
      "image": "ghcr.io/dispatcharr/dispatcharr:0.13.0",
      "isMain": true,
      "internalPort": 9191,
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/data",
          "containerPath": "/data"
        }
      ]
    }
  ]
}
