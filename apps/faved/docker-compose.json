{
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "schemaVersion": 2,
  "services": [
    {
      "name": "faved",
      "image": "denho/faved:2.5.0",
      "isMain": true,
      "internalPort": 80,
      "environment": [
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/storage",
          "containerPath": "/var/www/html/storage"
        }
      ]
    }
  ]
}
