{
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "schemaVersion": 2,
  "services": [
    {
      "name": "byparr",
      "image": "ghcr.io/thephaseless/byparr:2.0.1",
      "isMain": true,
      "internalPort": "8191",
      "environment": [
        {
          "key": "LOG_LEVEL",
          "value": "${BYPARR_LOG_LEVEL-info}"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ]
    }
  ]
}
