{
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "schemaVersion": 2,
  "services": [
    {
      "name": "razor-miniflux-restricted",
      "image": "romesc/razorminiflux:1767033274",
      "isMain": true,
      "internalPort": 8080,
      "environment": [
        {
          "key": "FRESHRSS_CONNECTIONSTRING",
          "value": "${FRESHRSS_CONNECTIONSTRING}"
        },
        {
          "key": "FRESHRSS_TABLE_PREFIX",
          "value": "${FRESHRSS_TABLE_PREFIX}"
        },
        {
          "key": "FRESHRSS_TOP_TABLE_PREFIX",
          "value": "${FRESHRSS_TOP_TABLE_PREFIX}"
        },
        {
          "key": "PAGE_SIZE",
          "value": "${PAGE_SIZE}"
        },
        {
          "key": "SERVER_PAGE_SIZE",
          "value": "${SERVER_PAGE_SIZE}"
        },
        {
          "key": "IS_TOP",
          "value": "${IS_TOP}"
        },
        {
          "key": "MARK_STARRED_READ",
          "value": "${MARK_STARRED_READ}"
        },
        {
          "key": "TZ",
          "value": "${TZ}"
        }
      ],
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/db",
          "containerPath": "/app/db"
        }
      ]
    }
  ]
}
