{
  "$schema": "https://schemas.runtipi.io/v2/dynamic-compose.json",
  "schemaVersion": 2,
  "services": [
    {
      "name": "tracktor",
      "image": "ghcr.io/javedh-dev/tracktor:0.0.1",
      "environment": [
        {
          "key": "NODE_ENV",
          "value": "production"
        },
        {
          "key": "PUBLIC_DEMO_MODE",
          "value": "false"
        }
      ],
      "isMain": true,
      "internalPort": 3000,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/appdata",
          "containerPath": "/data",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ]
    }
  ]
}