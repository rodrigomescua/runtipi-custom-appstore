{
  "services": [
    {
      "name": "tracktor",
      "image": "ghcr.io/javedh-dev/tracktor:0.0.1",
      "environment": {
        "NODE_ENV": "production",
        "PUBLIC_DEMO_MODE": "false"
      },
      "isMain": true,
      "internalPort": 3000,
      "volumes": [
        {
          "hostPath": "${APP_DATA_DIR}/data/appdata",
          "containerPath": "/data",
          "readOnly": false,
          "shared": false,
          "private": false
        }
      ]
    }
  ]
}