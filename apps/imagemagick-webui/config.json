{
  "$schema": "https://schemas.runtipi.io/v2/app-info.json",
  "name": "ImageMagick WebGUI",
  "id": "imagemagick-webui",
  "available": true,
  "short_desc": "Modern web interface for ImageMagick with AI-powered image processing",
  "author": "PrzemekSkw",
  "port": 8848,
  "categories": ["utilities", "media"],
  "description": "ImageMagick WebGUI is a beautiful, modern web interface for image processing. Features include drag & drop uploads, batch processing, AI-powered background removal, real-time preview, and support for multiple image formats (JPG, PNG, WebP, GIF, TIFF, PDF).",
  "tipi_version": 1,
  "version": "1.0.9",
  "source": "https://github.com/PrzemekSkw/imagemagick-webui",
  "exposable": true,
  "dynamic_config": true,
  "supported_architectures": ["amd64"],
  "created_at": 1734375600000,
  "updated_at": 1734375600000,
  "form_fields": [
    {
      "type": "random",
      "label": "Secret Key",
      "required": true,
      "env_variable": "SECRET_KEY",
      "min": 32,
      "encoding": "hex"
    },
    {
      "type": "random",
      "label": "JWT Secret",
      "required": true,
      "env_variable": "JWT_SECRET",
      "min": 32,
      "encoding": "hex"
    },
    {
      "type": "boolean",
      "label": "Require Login",
      "required": false,
      "env_variable": "REQUIRE_LOGIN",
      "default": "false"
    },
    {
      "type": "text",
      "label": "Max Upload Size (MB)",
      "required": false,
      "env_variable": "MAX_UPLOAD_SIZE_MB",
      "default": "100",
      "min": 1,
      "max": 20
    }
  ]
}
